{
  "name": "@fugood/apollo-link-firebase",
  "version": "1.1.1",
  "description": "Query Firebase in GraphQL with Apollo",
  "author": {
    "name": "wwwy3y3",
    "email": "wwwy3y3@gmail.com"
  },
  "files": [
    "lib"
  ],
  "main": "lib/bundle.umd.js",
  "module": "./lib/index.js",
  "jsnext:main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "keywords": [
    "apollo",
    "apollo-link",
    "graphql",
    "firebase"
  ],
  "scripts": {
    "lint": "tslint -c tslint.json src/**/*.ts",
    "bundle": "./node_modules/.bin/webpack --config webpack.config.prod.js",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --timeout 10000 --require ts-node/register --recursive --exit \"test/**/*.spec.ts\"",
    "clean": "rimraf lib",
    "prebuild": "npm run clean",
    "build": "tsc -p ./tsconfig.release.json --pretty",
    "postbuild": "npm run bundle",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js --mode development"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/faker": "^4.1.2",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.1",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/sinon": "^9.0.0",
    "@types/webpack": "^4.39.1",
    "@types/webpack-env": "1.15.2",
    "antd": "^4.0.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-schema": "^1.2.4",
    "apollo-utilities": "^1.3.2",
    "chai": "^4.2.0",
    "faker": "^4.1.0",
    "firebase": "^7.0.0",
    "graphql": "^15.0.0",
    "graphql-anywhere": "^4.2.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^6.0.0",
    "lodash": "^4.17.11",
    "mocha": "^8.0.1",
    "nyc": "^15.0.0",
    "react": "^16.8.6",
    "react-addons-test-utils": "^15.6.2",
    "react-apollo": "3.1.5",
    "react-dom": "^16.8.6",
    "rimraf": "^3.0.0",
    "sinon": "^9.0.0",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^6.0.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "repository": "Canner/apollo-link-firebase",
  "license": "MIT",
  "peerDependencies": {
    "firebase": ">= 4"
  }
}
